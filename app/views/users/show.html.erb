<h1><%= @user.user_name %></h1>

<h3>List of all my conspiracies</h3>

<ul>
  <% @user.conspiracies.each do |conspiracy| %>
    <li>
      <%= link_to user_conspiracy_path(@user, conspiracy) do %>
        <%= conspiracy.title %>
      <% end %>
      - <%= conspiracy.content %>
    </li>
  <% end %>
</ul>
